  "optionalDependencies": {
    "fsevents": "^2.1.2",
    "snyk": "^1.381.1"
  },
  "repository": "https://github.com/wevote/webapp.git",
  "pre-commit": [
    "test",
    "lint"
  ],
  "snyk": true
}
