    "react-helmet": "^5.2.0",
    "react-iframe-resizer-super": "^0.2.2",
    "react-intl": "^2.7.2",
    "react-mixin": "^3.1.1",
    "react-nps-input": "^0.3.0",
    "react-pikaday": "^0.5.1",
    "react-player": "^1.6.6",
