    "babel-eslint": "^7.1.1",
    "babel-plugin-object-assign": "^1.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "blue-tape": "^1.0.0",
    "browser-sync": "^2.26.3",
