{
  "name": "wevoteusa",
  "version": "1.0.0",
  "description": "We Vote front end application",
  "private": true,
  "main": "server.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "dynamic-import-node-babel-7",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "babel-plugin-styled-components",
        {
          "displayName": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind",
      "@babel/plugin-transform-runtime",
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/core",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "core"
      ],
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/icons",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "icons"
      ]
    ]
  },
  "react-svg-icons": {
    "assetPath": "src/img/global/svg-icons"
  },
  "scripts": {
    "lint": "eslint --format stylish --ext .jsx --ext .js src/js",
    "lint-css": "gulp lint-css",
    "test": "npm run lint && npm run lint-css",
    "start": "webpack-dev-server",
    "start-https": "webpack-dev-server --https --cert ./src/cert/server.crt --key ./src/cert/server.key ",
    "start-https-singleBundle": "BUNDLE=SINGLE webpack-dev-server --https --cert ./src/cert/server.crt --key ./src/cert/server.key",
    "start-prod-local": "node ./server-prod-local.js",
    "start-profile": "node --inspect-brk ./node_modules/.bin/gulp",
    "prod": "NODE_ENV=production webpack -p",
    "build": "webpack -p",
    "dev-singleBundle": "BUNDLE=SINGLE NODE_ENV=development webpack -p",
    "prod-singleBundle": "BUNDLE=SINGLE NODE_ENV=production webpack -p",
    "build-dev": "NODE_ENV=development gulp build",
    "deps": "npm run deps:missing && npm run deps:extra",
    "deps:missing": "dependency-check package.json",
    "deps:extra": "dependency-check package.json --extra --no-dev --ignore",
    "build:doc": "doctoc --github --title \"## Contents\" ./",
    "autoTest": " mocha --require @babel/register --require tests/component/index.js \"./tests/component/tests.js\" && npm run lint",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@wdio/concise-reporter": "^6.4.0",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "d3-geo": "^1.11.9",
    "es6-promise": "^4.2.8",
    "flux": "^3.1.3",
    "font-awesome": "^4.7.0",
    "history": "^3.2.1",
    "iframe-resizer": "^3.6.6",
    "jquery": "^3.5.1",
    "keymirror": "^0.1.1",
    "lodash-es": "^4.17.15",
    "lodash.assign": "^4.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "object-assign": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-copy-to-clipboard": "^5.0.2",
    "react-delay-render": "^0.1.2",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-iframe-resizer-super": "^0.2.2",
    "react-imported-component": "^6.3.5",
    "react-loadable": "^5.5.0",
    "react-mixin": "^3.1.1",
    "react-phone-number-input": "^2.5.1",
    "react-player": "^1.14.2",
    "react-pure-render": "^1.0.2",
    "react-router": "^3.2.5",
    "react-router-dom": "^5.2.0",
    "react-router-scroll": "^0.4.4",
    "react-share": "^4.1.0",
    "react-simple-maps": "^2.0.0",
    "react-slick": "^0.25.2",
    "react-stripe-elements": "^4.0.2",
    "react-svg": "^7.2.14",
    "react-text-truncate": "^0.16.0",
    "react-textarea-autosize": "^7.1.2",
    "react-toastify": "^3.4.3",
    "react-toggle": "^4.1.1",
    "react-transition-group": "^2.9.0",
    "react-usa-map": "^1.4.0",
    "styled-components": "^4.4.1",
    "superagent": "^3.8.3",
    "unused-webpack-plugin": "^2.4.0",
    "wdio": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-do-expressions": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.7",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-object-assign": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "@babel/types": "^7.7.4",
    "@wdio/cli": "^6.0.1",
    "@wdio/local-runner": "^5.23.0",
    "@wdio/mocha-framework": "^6.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babelify": "^10.0.0",
    "blue-tape": "^1.0.0",
    "browser-sync": "^2.26.12",
    "browserify": "^13.3.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.0",
    "del": "^2.2.0",
