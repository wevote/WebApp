    "keymirror": "^0.1.1",
    "lodash": "^4.17.11",
    "lodash.assign": "^4.2.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "node-sass": "^4.10.0",
    "npm": "^6.4.1",
    "object-assign": "^4.0.1",
    "popper.js": "^1.14.5",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-bootstrap": "^1.0.0-beta.2",
    "react-copy-to-clipboard": "^5.0.1",
